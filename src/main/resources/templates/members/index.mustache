{{>layouts/header}}

<table class="table">
    <thead>
    <tr>
        <th scope="col">id</th>
        <th scope="col">email</th>
        <th scope="col">password</th>
    </tr>
    </thead>
    <tbody>
    {{#members}}
        <tr>
            <td>{{id}}</td>
            <td><a href="/members/{{id}}">{{email}}</a></td>
            <td>{{password}}</td>
        </tr>
    {{/members}}
    </tbody>
</table>
<a href="members/new">New Member</a>
{{>layouts/footer}}
